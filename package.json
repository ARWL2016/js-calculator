    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-latest": "^6.22.0",
    "babel-register": "^6.23.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.27.3",
    "eslint": "^3.15.0",
